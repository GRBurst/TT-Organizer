<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.grburst.ttorganizer"
    android:versionCode="1"
    android:versionName="1.0">
    <uses-sdk android:targetSdkVersion="16" android:minSdkVersion="14"/>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:description="@string/app_desc">
        <activity android:name="com.grburst.ttorganizer.TTOrganizer"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.grburst.ttorganizer.activities.EventsActivity"
            android:label="Events">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.grburst.ttorganizer.activities.EventDetailActivity"
            android:label="EventDetail">
        </activity>
        <activity android:name="com.grburst.ttorganizer.activities.CalculatorActivity"
            android:label="Calculator">

        </activity>
        <activity android:name="com.grburst.ttorganizer.activities.LeagueRankingActivity"
            android:label="LeagueRanking">
        </activity>
        <activity android:name="com.grburst.ttorganizer.activities.ClubRankingActivity"
            android:label="ClubRanking">
        </activity>
    </application>
</manifest>
